{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "FlexStorage": "Information"
    }
  },
  "AWS": {
    "Region": "us-west-2",
    "S3": {
      "DeepArchiveBucket": "flexstorage-deep-archive-prod",
      "FlexibleBucket": "flexstorage-flexible-prod",
      "UseHttp": false,
      "ForcePathStyle": false,
      "DisableHostPrefixInjection": false,
      "MaxErrorRetry": 3,
      "UseDualstackEndpoint": false,
      "UseAccelerateEndpoint": false,
      "DisableLogging": true
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Database=flexstorage_prod;Username=postgres;Password=postgres"
  }
}